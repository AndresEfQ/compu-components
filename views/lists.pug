extends layout 

block content 

  div(class='container overlay')
    h1 #{title}

    div
      ul(class="list-group")
        each element in list 
          li(class="list-group-item d-flex justify-content-between align-items-center")  #{element.name}
            span(class="badge badge-primary badge-pill") #{element.quantiry}

        else 
          p There are no #{title}
